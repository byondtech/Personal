{
    "extends": ["next/core-web-vitals", "plugin:react/recommended"],
    "plugins": ["react"],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/prop-types": "warn",
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": "off"
    },
    "overrides": [
      {
        "files": ["next.config.js"],
        "env": {
          "node": true
        },
        "rules": {
          "no-undef": "off"
        }
      }
    ]
  }